

@{
    ViewBag.Title = "Results";
    Layout = "~/Views/shared/_Layout.cshtml";
}
@model IEnumerable<Vision_Project.Models.PartyVoteResult>

@{
    ViewBag.Title = "Election Results";
}



<table class="table table-striped">
    <thead>
        <tr>
            <th>Flag</th>
            <th>Shortcode</th>
            <th>Candidate Name</th>
            <th>Party Name</th>
            <th>Leader</th>
            <th>Vote Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model)
        {
            <tr>
                <td>
                    <img src="@Url.Content( result.Flagimg)" alt="Party Flag" width="50" height="30" />
                </td>
                <td>@result.Shortcode</td>
                <td>@result.CandidateName</td>
                <td>@result.PartyName</td>
                <td>@result.Leader</td>
                <td>@result.VoteCount</td>
            </tr>
        }
    </tbody>
</table>
