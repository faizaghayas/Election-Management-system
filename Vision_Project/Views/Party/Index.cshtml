                    <script>
                        const ctx = document.getElementById('performanceLine').getContext('2d');

                        // Fetch dynamic data (you can replace this with an actual API call)
                        function fetchData() {
                            return {
                                labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July'],
                                datasets: [{
                                    label: 'Performance',
                                    data: [23, 90, 89, 67, 77, 89, 12], // Replace with your dynamic data
                                    borderColor: 'rgba(75, 192, 192, 1)',
                                    borderWidth: 2,
                                    fill: false
                                }]
                            };
                        }

                        // Create the chart
                        const performanceLineChart = new Chart(ctx, {
                            type: 'line',
                            data: fetchData(),
                            options: {
                                responsive: true,
                                plugins: {
                                    legend: {
                                        display: true,
                                        position: 'top',
                                    }
                                },
                                scales: {
                                    x: {
                                        display: true,
                                        title: {
                                            display: true,
                                            text: 'Months'
                                        }
                                    },
                                    y: {
                                        display: true,
                                        title: {
                                            display: true,
                                            text: 'Performance'
                                        }
                                    }
                                }
                            }
                        });

                        // Update the chart with new data (if needed)
                        function updateChart() {
                            const newData = fetchData();
                            performanceLineChart.data = newData;
                            performanceLineChart.update();
                        }

                        // Example: Update the chart every 5 seconds with new data
                        setInterval(updateChart, 5000);
                    </script>

