    <script src="~/Content/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#datepickerStart').datepicker({
                startDate: new Date(),
                autoclose: true,
                todayHighlight: true
            });

            $('#datepickerEnd').datepicker({
                startDate: new Date(),
                autoclose: true,
                todayHighlight: true
            });

        });

        $(function () {
            $("#datepicker").datepicker({
                dateFormat: 'yy-mm-dd' // You can adjust the date format as needed
            });
        });


    </script>
    <script src="~/Content/assets/scripts/sweetalert.min.js"></script>

    @if (TempData["SucessMessage"] != null)
    {
        <script>
 swal({
                title: "@TempData["SucessMessage"]", icon: "success", button: "ok!",
            })</script>
    }
    @if (TempData["WarningMessage"] != null)
    {
        <script>
swal({
               title: "@TempData["WarningMessage"]", icon: "warning", button: "ok!",
           })</script>
    }
