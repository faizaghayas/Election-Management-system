@model IEnumerable<Vision_Project.Models.UserInfo>

@{
    ViewBag.Title = "UserInfo";
    Layout = "~/Views/shared/Adminlayout.cshtml";
}




<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">User Info </h4>
                <p class="card-description">

                </p>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                                <th>Father Name</th>
                                <th>Phone</th>
                                <th>Gender</th>
                                <th>Date Of Birth</th>
                                <th>Address</th>
                                <th>Cnic No</th>
                                <th>Family No</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@Html.DisplayFor(modelItem => item.Id)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Name)</td>
                                    <td>@Html.DisplayFor(modelItem => item.FatherName)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Phone)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Gender)</td>
                                    <td>@Html.DisplayFor(modelItem => item.DOB)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Address)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Cnic_No)</td>
                                    <td>@Html.DisplayFor(modelItem => item.Family_No)</td>

                                    <td><label class="badge badge-danger"></label></td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>